{
  "name": "fluent-reader",
  "version": "1.2.9",
  "description": "Modern desktop RSS reader",
  "main": "./dist/electron.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "electron": "electron ./dist/electron.js",
    "start": "npm run build && npm run electron",
    "format": "prettier --write .",
    "package-win": "electron-builder -w appx:x64 && electron-builder -w appx:ia32 && electron-builder -w appx:arm64",
    "package-win-ci": "electron-builder -w --x64 -p never && electron-builder -w --ia32 -p never",
    "package-mac": "electron-builder --mac --x64",
    "package-mas": "bash build/resignAndPackage.sh",
    "package-linux": "electron-builder --linux --x64 -p never"
  },
  "keywords": [],
  "author": "Haoyuan Liu",
  "license": "BSD-3-Clause",
  "repository": "github:yang991178/fluent-reader",
  "devDependencies": {
    "@fluentui/react": "^8.123.2",
    "@types/lovefield": "^2.1.7",
    "@types/nedb": "^1.8.16",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "electron-react-devtools": "^0.5.3",
    "electron-store": "^10.1.0",
    "electron-window-state": "^5.0.3",
    "font-list": "^1.5.1",
    "html-webpack-plugin": "^5.6.3",
    "js-md5": "^0.8.3",
    "lovefield": "^2.1.12",
    "nedb": "^1.8.0",
    "prettier": "3.6.2",
    "process": "^0.11.10",
    "qrcode.react": "^4.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intl-universal": "^2.12.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "reselect": "^5.1.1",
    "rss-parser": "^3.13.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "node-polyfill-webpack-plugin": "^4.1.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
